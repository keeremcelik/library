@model Entity.Publisher
@{
ViewData["Title"] = "Edit Publisher ";
}
<div class="container">
    
<form asp-action="Edit" asp-controller="Publisher" method="POST" >
    <input type="hidden" asp-for="PublisherId">
 <div class="form-group">
   <label asp-for="Name">Name</label>
    <input asp-for="Name" class="form-control" placeholder="Please enter publisher name.">
  </div>
  
 
<button type="submit" class="btn btn-primary">Save</button>
</form>
</div>