@model IEnumerable<Book>
@{
    ViewData["Title"] = "User Unread ";
}
<div class="container my-4">
    <div class="row">
        <div class="col-md-3">
            <partial name="_UserPageMenu" model="Model" />
        </div>
        <div class="col-md-9">
            <div class="jumbotron jumbotron-fluid">
                <div class="container">
                    <h1 class="display-4">Unread Books</h1>
                    <p class="lead">Unread books are listed here.</p>
                </div>
            </div>        
            <partial name="_HomeList" model="Model" />
        </div>        
    </div>  
</div>